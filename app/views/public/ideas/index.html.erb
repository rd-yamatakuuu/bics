<div class='container'>
  
  <div class='row'>
    <h1>ideas</h1>
  </div>
  
  <div class='ro'>
    <% @ideas.each do |idea| %>
      <div>
        <%= idea.title %>
        <%= attachment_image_tag idea, :icon, height:'120', fallback:'no_image.png' %> 
      </div>
    <% end %>
  </div>
  
</div>